<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.danielphillips.watson911">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">



        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Information">
            <intent-filter>
                <action android:name="android.intent.action.INFORMATION" />

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".EmergencyInformation">
            <intent-filter>
                <action android:name="android.intent.action.EMGRINFORMATION" />

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".WaitingOnEmergResponder">
            <intent-filter>
                <action android:name="android.intent.action.WAITINGONEMERG" />

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".AddNotes">
            <intent-filter>
                <action android:name="android.intent.action.ADDNOTES" />

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyBG1NwLPrHQDLfdrOtmysP29ILs61Lx0j0"/>
        AIzaSyCSClp21Y_ywVIebbxYjrmYAnBe9CDAj8E
    </application>

</manifest>